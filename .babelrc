{
    "presets": [
        [
            "env",
            {
                "targets": {
                    "node": 7
                },
                "useBuiltIns": true
            }
        ],
        "stage-0",
        "react"
    ],
    "env": {
        "development": {
            "plugins": [
                [
                    "flow-runtime",
                    {
                        "assert": true,
                        "annotate": true
                    }
                ]
            ]
        }
    }
}