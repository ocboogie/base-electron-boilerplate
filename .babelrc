{
  "presets": [
    [
      "env",
      {
        "targets": {
          "node": 7
        },
        "useBuiltIns": true
      }
    ],
    "stage-0",
    "react"
  ],
  "env": {
    "development": {
      "plugins": [
        "transform-class-properties",
        "transform-es2015-classes",
        [
          "flow-runtime",
          {
            "assert": true,
            "annotate": true
          }
        ]
      ]
    }
  }
}
