{
  "name": "base-electron-boilerplate",
  "version": "0.0.1",
  "description": "This is a Electron boilerplate using Electron, React, Redux, React Router, Babel, Webpack and Flow",
  "main": "dist/main.js",
  "repository": "git@github.com:ocboogie/base-electron-boilerplate.git",
  "author": "ocboogie <boogie@mikulec.com>",
  "license": "MIT",
  "scripts": {
    "build": "concurrently -n \"main,renderer\" \"npm run build-main\" \"npm run build-renderer\"",
    "build-renderer": "cross-env NODE_ENV=development node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack/webpack.config.renderer.js --colors",
    "build-main": "cross-env NODE_ENV=development node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack/webpack.config.main.js --colors",
    "start": "electron .",
    "dev": "cross-env START_HOT=true npm run start-renderer-dev",
    "start-renderer-dev": "cross-env NODE_ENV=development node --trace-warnings -r babel-register ./node_modules/webpack-dev-server/bin/webpack-dev-server --config webpack/webpack.config.renderer.js",
    "start-main-dev": "cross-env HOT=true NODE_ENV=development nodemon --watch app/main --exec \"electron -r babel-register ./app/main/index\"",
    "lint": "eslint --cache --format=node_modules/eslint-formatter-pretty .",
    "lint-fix": "npm run lint -- --fix",
    "flow-typed": "rimraf flow-typed/npm && flow-typed install --overwrite",
    "flow": "flow"
  },
  "devDependencies": {
    "autodll-webpack-plugin": "^0.2.1",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-flow-runtime": "^0.11.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "concurrently": "^3.5.0",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "electron": "^1.7.8",
    "electron-devtools-installer": "^2.2.0",
    "eslint": "^4.7.2",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-formatter-pretty": "^1.3.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-flowtype": "^2.36.0",
    "eslint-plugin-flowtype-errors": "^3.3.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.4.0",
    "file-loader": "^0.11.2",
    "flow-bin": "^0.55.0",
    "flow-runtime": "^0.14.0",
    "flow-typed": "^2.1.5",
    "html-webpack-plugin": "^2.30.1",
    "nodemon": "^1.12.1",
    "rimraf": "^2.6.2",
    "style-loader": "^0.18.2",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "url-loader": "^0.5.9",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.8.2",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "devtron": "^1.4.0",
    "electron-debug": "^1.4.0",
    "history": "^4.7.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "source-map-support": "^0.4.18"
  }
}
